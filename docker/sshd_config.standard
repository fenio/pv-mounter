# /etc/ssh/sshd_config
HostKey /etc/ssh/ve_keys/ssh_host_rsa_key
HostKey /etc/ssh/ve_keys/ssh_host_ecdsa_key
HostKey /etc/ssh/ve_keys/ssh_host_ed25519_key
PermitRootLogin no
PasswordAuthentication no
ChallengeResponseAuthentication no
AuthorizedKeysFile none
AuthorizedKeysCommand /sshkey.sh
AuthorizedKeysCommandUser ve

UsePAM no
PrintMotd no
PidFile none
Subsystem sftp /usr/lib/openssh/sftp-server

# Enable logging
LogLevel VERBOSE
